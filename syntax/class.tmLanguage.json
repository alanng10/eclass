{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Class",
	"patterns": [
		{
			"include": "#Comment"
		},
		{
			"include": "#Keyword"
		},
		{
			"include": "#IntValue"
		},
		{
			"include": "#StringValue"
		}
	],
	"repository": {
		"Keyword": {
			"patterns": [{
				"name": "keyword.class",
				"match": "(?<=^|[^A-Za-z0-9_])(class|get|set|this|base|new|share|cast|null|true|false|sign|bit|return|inf|while|prudate|probate|precate|private)(?=$|[^A-Za-z0-9_])"
			}]
		},
		"Comment": {
			"patterns": [{
				"name": "comment.class",
				"match": "#(.*)"
			}]
		},
		"IntValue": {
			"patterns": [{
				"name": "constant.numeric.class",
				"match": "(?<=^|[^A-Za-z0-9_])(([0-9]+)|(0h([a-f0-9]+))|(0s[np]([0-9]+))|(0hs[np]([a-f0-9]+)))(?=$|[^A-Za-z0-9_])"
			}]
		},
		"StringValue": {
			"patterns": [{
				"name": "string.quoted.class",
				"match": "\"((\\\\\")|([^\"\\n]))*([\"]?)"
			}]
		}
	},
	"scopeName": "source.class"
}