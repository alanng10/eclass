{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Class",
	"patterns": [
		{
			"include": "#Comment"
		},
		{
			"include": "#KeywordClassName"
		},
		{
			"include": "#Keyword"
		},
		{
			"include": "#IntValue"
		},
		{
			"include": "#StringValue"
		}
	],
	"repository": {
		"Keyword": {
			"patterns": [{
				"name": "keyword.class",
				"match": "(?<=^|[^A-Za-z0-9_])(class|get|set|this|base|new|share|cast|null|true|false|sign|bit|return|inf|while|prudate|probate|precate|private)(?=$|[^A-Za-z0-9_])"
			}]
		},
		"KeywordClassName": {
			"patterns": [{
				"begin": "(?<=^|[^A-Za-z0-9_])(new|share|cast|prudate|probate|precate|private)(?=$|[^A-Za-z0-9_])",
				"beginCaptures": {
					"1": {
						"name": "keyword.class"
					}
				},
				"end": "((?!((?<=^|[^A-Za-z0-9_])(class|get|set|this|base|new|share|cast|null|true|false|sign|bit|return|inf|while|prudate|probate|precate|private)(?=$|[^A-Za-z0-9_])))([A-Za-z][A-Za-z0-9_]*))|.",
				"endCaptures": {
					"4" : {
						"name": "entity.name.type.class"
					}
				},
				"patterns": [
					{
						"include": "#Comment"
					},
					{
						"match": "[ ]+"
					}
				]
			}]
		},
		"ClassName": {
			"patterns": [{
				"match": "(?<=^|[^A-Za-z0-9_])(((class)[ ]+(?!((?<=^|[^A-Za-z0-9_])(class|get|set|this|base|new|share|cast|null|true|false|sign|bit|return|inf|while|prudate|probate|precate|private)(?=$|[^A-Za-z0-9_])))([A-Za-z][A-Za-z0-9_]*)([ ]*:[ ]*(?!((?<=^|[^A-Za-z0-9_])(class|get|set|this|base|new|share|cast|null|true|false|sign|bit|return|inf|while|prudate|probate|precate|private)(?=$|[^A-Za-z0-9_])))([A-Za-z][A-Za-z0-9_]*))?)|((new|share|cast|prudate|probate|precate|private)[ ]+(?!((?<=^|[^A-Za-z0-9_])(class|get|set|this|base|new|share|cast|null|true|false|sign|bit|return|inf|while|prudate|probate|precate|private)(?=$|[^A-Za-z0-9_])))([A-Za-z][A-Za-z0-9_]*))|((?!((?<=^|[^A-Za-z0-9_])(class|get|set|this|base|new|share|cast|null|true|false|sign|bit|return|inf|while|prudate|probate|precate|private)(?=$|[^A-Za-z0-9_])))([A-Za-z][A-Za-z0-9_]*)[ ]+(?!((?<=^|[^A-Za-z0-9_])(class|get|set|this|base|new|share|cast|null|true|false|sign|bit|return|inf|while|prudate|probate|precate|private)(?=$|[^A-Za-z0-9_])))([A-Za-z][A-Za-z0-9_]*)))(?=$|[^A-Za-z0-9_])",
				"captures": {
					"3" : { "name": "keyword.class" },
					"6" : { "name": "entity.name.type.class" },
					"10" : { "name": "entity.name.type.class" },
					"12" : { "name": "keyword.class" },
					"15" : { "name": "entity.name.type.class" },
					"19" : { "name": "entity.name.type.class" }
				}
			}]
		},
		"Comment": {
			"patterns": [{
				"name": "comment.class",
				"match": "#(.*)"
			}]
		},
		"IntValue": {
			"patterns": [{
				"name": "constant.numeric.class",
				"match": "(?<=^|[^A-Za-z0-9_])([0-9]+)(?=$|[^A-Za-z0-9_])"
			}]
		},
		"StringValue": {
			"patterns": [{
				"name": "string.quoted.class",
				"match": "\"(((\\\\\\\\)|(\\\\\")|(\\\\n)|([^\"\\n\\\\]))*)\""
			}]
		}
	},
	"scopeName": "source.class"
}