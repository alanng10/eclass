{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Class",
	"patterns": [
		{
			"include": "#Comment"
		},
		{
			"include": "#ClassName"
		},
		{
			"include": "#Keyword"
		},
		{
			"include": "#IntValue"
		},
		{
			"include": "#StringValue"
		}
	],
	"repository": {
		"Keyword": {
			"patterns": [{
				"name": "keyword.class",
				"match": "\\b(class|get|set|this|base|new|share|cast|null|true|false|sign|bit|return|inf|while|prudate|probate|precate|private)\\b"
			}]
		},
		"ClassName": {
			"patterns": [{
				"match": "\\b(((class)[\\s]+(?!(\\b(class|get|set|this|base|new|share|cast|null|true|false|sign|bit|return|inf|while|prudate|probate|precate|private)\\b))([A-Za-z][A-Za-z0-9_]*)([\\s]*:[\\s]*(?!(\\b(class|get|set|this|base|new|share|cast|null|true|false|sign|bit|return|inf|while|prudate|probate|precate|private)\\b))([A-Za-z][A-Za-z0-9_]*))?)|((new|share|cast|prudate|probate|precate|private)[\\s]+(?!(\\b(class|get|set|this|base|new|share|cast|null|true|false|sign|bit|return|inf|while|prudate|probate|precate|private)\\b))([A-Za-z][A-Za-z0-9_]*))|((?!(\\b(class|get|set|this|base|new|share|cast|null|true|false|sign|bit|return|inf|while|prudate|probate|precate|private)\\b))([A-Za-z][A-Za-z0-9_]*)[\\s]+(?!(\\b(class|get|set|this|base|new|share|cast|null|true|false|sign|bit|return|inf|while|prudate|probate|precate|private)\\b))([A-Za-z][A-Za-z0-9_]*)))\\b",
				"captures": {
					"3" : { "name": "keyword.class" },
					"6" : { "name": "entity.name.type.class" },
					"10" : { "name": "entity.name.type.class" },
					"12" : { "name": "keyword.class" },
					"15" : { "name": "entity.name.type.class" },
					"19" : { "name": "entity.name.type.class" }
				}
			}]
		},
		"Comment": {
			"patterns": [{
				"name": "comment.class",
				"match": "#(.*)"
			}]
		},
		"IntValue": {
			"patterns": [{
				"name": "constant.numeric.class",
				"match": "\\b([0-9]+)\\b"
			}]
		},
		"StringValue": {
			"patterns": [{
				"name": "string.quoted.class",
				"match": "\"(((\\\\\\\\)|(\\\\\")|(\\\\n)|([^\"\\n\\\\]))*)\""
			}]
		}
	},
	"scopeName": "source.class"
}